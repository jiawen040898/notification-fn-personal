{
    "name": "notification-fn",
    "version": "2.11.0",
    "author": "Pulsifi <dev-team@pulsifi.me>",
    "license": "UNLICENSED",
    "description": "Notification AWS Lambda",
    "private": true,
    "engines": {
        "node": ">=20.0.0",
        "yarn": ">=1.22.19"
    },
    "scripts": {
        "prepare": "is-ci || husky install",
        "build": "yarn install --frozen-lockfile --prefer-offline && yarn install --frozen-lockfile --prefer-offline --cwd layer/nodejs",
        "format": "pretty-quick --staged && prettier --loglevel \"warn\" --debug-check \"src/**/*.ts\" \"tests/**/*.ts\"",
        "lint": "yarn format && eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
        "test": "jest --watch",
        "test:ci": "jest --ci --coverage",
        "cdk": "cdk",
        "biome:format": "biome format --write ./cdk",
        "biome:lint": "biome lint ./cdk",
        "biome:check": "biome check --apply ./cdk"
    },
    "devDependencies": {
        "aws-cdk": "2.141.0",
        "aws-cdk-lib": "2.141.0",
        "@pulsifi/custom-aws-cdk-lib": "1.14.1",
        "constructs": "10.3.0",
        "@mrgrain/cdk-esbuild": "5.1.0",
        "esbuild-node-externals": "1.13.1",
        "esbuild-plugin-copy": "2.1.1",
        "esbuild-plugin-time": "1.0.0",
        "esbuild-plugin-typescript-decorators": "0.1.0",
        "@actions/core": "1.10.1",
        "@biomejs/biome": "1.7.3",
        "@pulsifi/fn": "2.3.0",
        "@pusher/push-notifications-server": "1.2.6",
        "axios": "^1.6.0",
        "liquidjs": "10.8.4",
        "pg": "8.11.3",
        "pusher": "5.2.0",
        "rfc2047": "4.0.1",
        "typeorm": "0.3.17",
        "@aws-sdk/client-ses": "^3.385.0",
        "@aws-sdk/client-sns": "^3.385.0",
        "@aws-sdk/client-sqs": "^3.385.0",
        "@aws-sdk/middleware-retry": "3.374.0",
        "@commitlint/cli": "17.6.7",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/npm": "11.0.1",
        "@types/aws-lambda": "8.10.134",
        "@types/cache-manager": "4.0.6",
        "@types/cache-manager-redis-store": "2.0.4",
        "@types/jest": "29.5.12",
        "@types/node": "20.11.19",
        "@types/pg": "8.11.0",
        "@types/rfc2047": "2.0.3",
        "@types/serverless": "3.12.22",
        "@types/uuid": "9.0.8",
        "@typescript-eslint/eslint-plugin": "6.2.1",
        "@typescript-eslint/parser": "6.2.1",
        "better-sqlite3": "8.6.0",
        "commitlint-plugin-function-rules": "2.0.2",
        "eslint": "8.46.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-import": "2.28.0",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "factory.ts": "1.4.1",
        "husky": "8.0.3",
        "is-ci": "3.0.1",
        "jest": "29.7.0",
        "jest-junit": "16.0.0",
        "lint-staged": "13.2.3",
        "mockdate": "3.0.5",
        "prettier": "2.8.8",
